<p id="notice"><%= notice %></p>
<div style="text-align: right"><%= link_to 'Help', admin_help_path %></div>

<h1>Questions</h1>
<br>

<%= link_to 'Universities', universities_path %> | 
<%= link_to 'Nominators', representatives_path %> | 
<%= link_to 'Students', students_path %> | 
<%= link_to 'Questions', questions_path %>
<br>
<br>

<table>
  <thead>
    <tr>
      <th>Multi</th>
      <th>Prompt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.multi %></td>
        <td><%= question.prompt %></td>
        <td><%= link_to 'Show', question %></td>
        <td><%= link_to 'Edit', edit_question_path(question) %></td>
        <td><%= link_to 'Delete', delete_question_path(question) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question', new_question_path %>
<br>
<%= link_to 'Back to Admin', admin_path %>