<h1>New Response</h1>
<% Question.all.each do |question| %>
  <tr>
	<% @question = question %>
    <%= render 'form', response: @response %>
	<p>
	<strong>Current Response:</strong>
	<%= @response.reply %>
	</p>
  </tr>
<% end %>

<%= link_to 'Done', student_responses_path(@student) %>
<br><br>
<%= link_to 'Back to Responses', student_responses_path %>

